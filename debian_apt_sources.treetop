grammar DebianAptSources
  rule list
    (repository_line / comment / spaces)+
  end
  rule repository_line
    ("deb " / "deb-src ") url  "\n"?
  end
  rule url
    "http://" (!"\n" .)+  <DebianSyntaxNode::SourceListUrl>
  end
  rule comment
    "#" (!"\n" .)+ "\n"
  end
  rule spaces
    ([\n \t])+
  end
end
